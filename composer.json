{
    "name": "achristodoulou/php-package",
    "description": "This php package can be used for testing packagist or satis.",
    "keywords": [
        "league"
    ],
    "homepage": "https://github.com/achristodoulou/php-package",
    "license": "MIT",
    "authors": [
        {
            "name": "Andreas Christodoulou",
            "role": "Developer"
        }
    ],
    "require": {
        "php" : ">=5.3.0"
    },
    "require-dev": {
        "phpunit/phpunit" : "4.*",
        "scrutinizer/ocular": "~1.1"
    },
    "autoload": {
        "psr-4": {
            "League\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "League\\Skeleton\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
